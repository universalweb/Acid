{
    "name": "acidjs",
    "company": "Universal Web",
    "version": "3.0.17",
    "description": "Acidjs is a robust, modern, modular, & async focused Javascript utility library.",
    "bugs": {
        "url": "https://github.com/universalweb/Acid/issues"
    },
    "homepage": "https://acidjs.com",
    "directories": {
        "doc": "docs",
        "source": "source",
        "build": "build",
        "npm": "npm",
        "test": "tests"
    },
    "readme": "README.md",
    "auto-changelog": {
        "commitLimit": false,
        "output": "CHANGELOG.md",
        "template": "./maintenance/keepachangelog.hbs",
        "unreleased": true
    },
    "keywords": [
        "Acid",
        "utility",
        "util",
        "modules",
        "stdlib"
    ],
    "license": "ISC",
    "main": "index.js",
    "maintainers": [
        {
            "email": "tommarchi@protonmail.com",
            "name": "Tom Marchi"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/universalweb/Acid.git",
        "directory": "npm"
    },
    "scripts": {
        "build": "node index.js",
        "compile": "production=true node index.js",
        "versionBump": "npm version patch -m \"Upgrade to %s\" --force && npm run npmVersion",
        "npmVersion": "cd ./package/ && npm version patch --force",
        "changelog": "npx auto-changelog -p --sort-commits date-desc && git add CHANGELOG.md",
        "pub": "npm update && npm run compile && npm run versionBump && npm run changelog && npm publish ./package/"
    },
    "dependencies": {
        "@babel/core": "^7.21.0",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/preset-env": "^7.20.2",
        "@microsoft/eslint-formatter-sarif": "^3.0.0",
        "auto-changelog": "^2.4.0",
        "body-parser": "1.20.1",
        "connect-livereload": "0.6.1",
        "daisyui": "^2.50.2",
        "docredux": "3.0.0",
        "eslint": "^8.34.0",
        "eslint-plugin-jsdoc": "^40.0.0",
        "express": "4.18.2",
        "magic-string": "0.29.0",
        "node-watch": "^0.7.3",
        "rollup": "3.17.2",
        "rollup-plugin-terser": "7.0.2",
        "tailwindcss": "^3.2.7",
        "testatron": "^2.1.0",
        "tiny-lr": "^2.0.0"
    },
    "devDependencies": {
        "eslint-plugin-import": "^2.27.5"
    }
}
